  eqdsk = 'eqdsk' !eqdsk input file
  netCDF_file1 = 'cql3d.nc' !cql3d distribution function file
  netCDF_file2 = 'cql3d.nc' !cql3d distribution function file
  root = 'none' !sum all modes

  !resolution and proc grid spec
  nmodesx = 128
  nmodesy = 128
  nprow = 32
  npcol = 32
  lmax = 3 !highest order bessel function retained

  !antenna parameters
  prfin = 2.5e6 !launched power [W]
  freqcy = 80.0e6 !frequency [Hz]
  nstrap = 2 !number of straps
  rant = 0.875 !location of the antenna in major radius R [m]
  yant = 0.0 !location of the antenna in height vs midplane Z [m]
